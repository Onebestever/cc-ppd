<body id="partner">
    <header>
            <nav>
                <ul>
                <a href="/"> <li><i class="fa fa-leaf fa" aria-hidden="true"> Home</i></li></a>
                <a href="/whoWeAre"> <li> <i class="fa fa-leaf fa" aria-hidden="true">Who We Are</i></li></a>
                <a href="/profile"> <li><i class="fa fa-leaf fa" aria-hidden="true"> Profile</i></li></a>
                <a href="/signup"> <li> <i class="fa fa-leaf fa" aria-hidden="true">Sign Up</i></li></a>
                <a href="/login"> <li><i class="fa fa-leaf fa" aria-hidden="true">Log In</i></li></a>
                <a href="/logout"> <li><i class="fa fa-leaf fa" aria-hidden="true">Log Out</i></li></a>
                <a href="/relationship"> <li><i class="fa fa-leaf fa" aria-hidden="true"> Add Viewers</i></li></a>
                <a href="/feed"> <li><i class="fa fa-leaf fa" aria-hidden="true"> View Feed</i></li></a>
                <a href="/chart"> <li><i class="fa fa-leaf fa" aria-hidden="true"> View charts</i></li></a>

            </nav>
        </section>
    </header>

        <main>

            <section class="leftSection">
                <h3><span class="fa fa-user"></span> Doctor</h3>
                <!-- render stuff if this user exist -->
                                <% if (user.local.email) { %>
                                    <p>
                                        <!-- <strong>id</strong>: <%= user._id %><br> -->
                                        <strong>First Name</strong>: <%= user.local.firstName %><br>
                                        <strong>Last Name</strong>: <%= user.local.lastName %><br>
                                        <!-- <strong>email</strong>: <%= user.local.date %><br> -->
                                        <!-- <strong>password</strong>: <%= user.local.password %> -->
                                    </p>
                <section class="imageHolder">
                    <img class="profileImage" src="/img/doctorProfile.jpeg">
                </section>
     </section>
             <section class="rightSection"> 
                       
                 <section class="logs">
                     <h2>Notes</h2>
                        
                                <img class="drFeed" src="/img/drFeed.jpg">
                    <ul class="messages">
                        <% for(var i=0; i<journalEntries.length; i++) {%>
                        <li class="message">
                                    Date:<%= journalEntries[i].date %>
                                    Stress Rate:<%= journalEntries[i].stressLevel %>
                                    DailyPost:<%= journalEntries[i].dailyPost %>
                                    User:<%= journalEntries[i].email %>
        

                                </li>
                                <% } %>
                                </ul>

                                    <% } else { %>
                                        <a href="/connect/local" class="btn btn-default">Connect Local</a>
                                    <% } %>
                      </section>
             </section>
         
    
                            <!-- LOCAL INFORMATION -->
    </main>
    <footer>
        <section class="img">
            <img class="ad" src="images/ad.jpg">
